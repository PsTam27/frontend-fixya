{
  "expo": {
    "name": "frontend-fixya",
    "slug": "frontend-fixya",
    "scheme": "frontend-fixya",
    "version": "1.0.0",
    "web": {
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      "expo-router",
      "expo-web-browser",
      "expo-font",
      ["expo-location",
        {
          "locationWhenInUsePermission": "Show current location on map."
        }]
    ],
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.lr56c.frontendfixya"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "600189a8-6d60-40b2-9dfa-51ddb67bf866"
      }
    }
  }
}
